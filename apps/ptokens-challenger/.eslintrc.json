{
  "extends": ["../../.eslintrc.json"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": ["@babel/plugin-syntax-import-assertions"]
    },
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "node/no-missing-import": "off",
    "node/no-unsupported-features/es-syntax": "off"
  }
}
